# --Terminal-- 3/23/2024 3:45:42 PM

#Permanently enable the terminal once the editor has been opened
if(GetEditMode)
	true ->inEditor
endif

if(GetKeyDown("BackQuote" 0) <-inEditor &&)
	@ToggleTerminal
endif

if(<-terminalOpen !)
	return
endif

if(<-errorCode)
	if(<-errorCode 1 eq)
		@PrintToOutput("ERROR: execution stopped unexpectedly. Likely an empty stack error.")
	endif
	0 ->errorCode
endif

@GetInput

:ParseAndProcess
	";" concat ->parseString
	"" ->currentWord
	
	true ->canBeNum
	true ->canBeWord
	false ->inString
	
	1 ->errorCode
	
	<-parseString StringToList ->parseList
	<-parseList 0 do
		<-parseList[i] ->char
		if(<-char DQ eq)
			PlaySound("Notify" 1)
			<-inString ! ->inString
		endif

		if(<-wordChars <-char ToLower ListContains !)
			false ->canBeWord
		endif
		if(<-numChars <-char ToLower ListContains !)
			false ->canBeNum
		endif
		
		if(<-canBeNum <-canBeWord || <-inString ||)
			<-currentWord <-char concat ->currentWord

		else if(<-currentWord StringLength)
			if(<-validString)
				<-currentWord dup StringLength 1 sub 1 swap SubString #dup @PrintToOutput
			else if(<-validNum)
				if(<-currentWord AsInt <-currentWord "0" eq ||)
					<-currentWord AsInt
				else if(<-currentWord AsFloat <-currentWord "0." eq <-currentWord "0.0" eq || ||)
					<-currentWord AsFloat
				else 
					2 ->errorCode
					@PrintToOutput("ERROR: invalid number format (" <-currentWord ")" concat3)
				endif endif
			else if(<-validWord)
				<-currentWord ToLower ->currentWord
				if(<-currentWord "<-" StartsWith)
					<-currentWord dup StringLength 2 sub 2 swap Substring @refread
				else if(<-currentWord "->" StartsWith)
					<-currentWord dup StringLength 2 sub 2 swap Substring @refwrite
				else if(<-apiTable{<-currentWord} dup StringLength)
					Refcall
				else
					pop 
					@PrintToOutput("ERROR: unknown operator (" <-currentWord ")" concat3)
					3 ->errorCode
				endif endif endif
			endif endif endif
			endif
			"" ->currentWord
			true ->canBeNum
			true ->canBeWord
			false ->inString
		endif
		
		<-canBeWord ->validWord
		<-canBeNum ->validNum
		<-inString ->validString
	loop
	
	@PrintAllToOutput
	if(<-errorCode 1 eq)
		0 ->errorCode
	endif

:PrintAllToOutput
	List ->outputs
	<-outputs 0 do
		<-outputs[i] ->value
		if(<-value GetType "STRING" eq)
			DQ <-value DQ concat3 @PrintToOutput
		else
			<-value AsString @PrintToOutput
		endif
	loop

:Once
	ShowRaster(false)
	false ->terminalOpen
	false ->inEditor
	"" ->inputString
	V2(90 90) ->*RL_cursorPos 
	V2(3 3) ->startCursor
	500 250 @ResizeRaster
	@BlitInputArea
	-1 ->scope
	@InitKeys
	@InitAPITable
	@InitVarSandbox
	@InitHelpTable
	
	"4RPL Terminal V0.0.1
By Qople
Updated March 2024
Type help for more"
	@PrintToOutput
	
:ToggleTerminal
	<-terminalOpen ! ->terminalOpen
	ShowRaster(<-terminalOpen)
	if(<-terminalOpen)
		RPLCamCopyPosAndRot
		false SetUnderCreeperEffectEnabled
		false SetMapZoomEnabled
		false SetMapPanEnabled
		false SetMapRotateEnabled
		SetRPLCam(true)
		RegisterForMsg("MSG_FrameAdvance" "CancelButKeepSelected")
	else
		false SetRPLCam
		true SetMapZoomEnabled
		true SetMapPanEnabled
		true SetMapRotateEnabled
		DeregisterForMsg("MSG_FrameAdvance")
	endif

:CancelButKeepSelected
	GetSelectedUnits ->selectedUnits
	Cancel
	<-selectedUnits 0 do
		<-selectedUnits[i] true SetUnitSelected
	loop

#Deals with managing string input, and displaying it accordingly on the raster.
:GetInput
	false ->charPrinted
	
	<-allKeys 0 do
		<-allKeys[i] ->keyData
		if(GetKeyDown(<-keyData[0] 0))
			true ->charPrinted
			<-keyData[1 GetKey("LeftShift" 0) GetKey("RightShift" 0) || add] ->char
			<-inputString <-char concat ->inputString
			# SendMsg("RL_PrintString" <-char)
			Cancel
		endif
	loop
	
	if(GetKey("Backspace" 0))
		<-backspaceTimer 1 add ->backspaceTimer
		#Allow holding backspace to continuously delete characters
		if(<-backspaceTimer 1 eq <-backspaceTimer 20 gt <-backspaceTimer 2 mod eq0 && ||)
			true ->charPrinted
			@Backspace
		endif
	else
		0 ->backspaceTimer
	endif
	
	if(GetKeyDown("Return" 0))
		"> " <-inputString concat @PrintToOutput
		<-inputString @ParseAndProcess
		"" ->inputString
		true ->charPrinted
	endif
	
	if(<-charPrinted)
		@BlitInputArea
	endif

#Blits the whole input area, text included, to the raster.
:BlitInputArea
	0 0 <-bottomSprite <-rasterWidth BlitRaster
	
	<-*RL_cursorPos ->outputPos #Save on the stack to reset after printing
	
	<-startCursor ->*RL_cursorPos
	
	<-inputString StringLength <-charsPerLine sub ->startIndex
	<-charsPerLine <-startIndex add <-charsPerLine min ->printLength
	
	SendMsg("RL_PrintString" "> " <-inputString <-startIndex 0 max <-printLength Substring concat)
	<-outputPos ->*RL_cursorPos

:PrintToOutput
	true ->*RL_enableLogging
	SendMsg("RL_PrintString" swap LF concat)
	false ->*RL_enableLogging
	if(<-*RL_cursorPos.y 3 lt)
		@ReprintOutput
	endif

:ReprintOutput
	ClearRaster(<-colorBlack)
	<-outputStartPos ->*RL_cursorPos
	<-*RL_printLog <-*RL_logLength min 0 do
		SendMsg("RL_PrintString" <-*RL_printLog[i])
	loop

#Updates some display-related settings that have to be changed whenever the raster size changes
#Then resizes the raster, clears it, and reprints the window.
:ResizeRaster
	->rasterHeight ->rasterWidth
	
	#The sprite for the bottom line
	V4(0 0 0 1) ->colorBlack
	V4(1 1 1 1) ->colorWhite
	<-rasterWidth <-*RL_lineHeight 2 add mul 0 do
		<-colorBlack
	loop
	<-rasterWidth 0 do
		<-colorWhite
	loop
	List ->bottomSprite
	
	<-rasterWidth <-rasterHeight <-*RL_lineHeight 2 add mul sub 0 do
		<-colorBlack
	loop
	List ->topSprite
	
	V4(3 3 <-rasterWidth <-*RL_letterSpacing sub <-rasterHeight <-*RL_lineHeight sub) ->*RL_wrapRect
	<-rasterWidth <-*RL_letterSpacing div 4 sub ->charsPerLine
	
	<-rasterWidth <-rasterHeight SetRasterSize
	ClearRaster(<-colorBlack)
	
	V2(3 <-rasterHeight <-*RL_lineHeight sub 3 sub) dup ->outputStartPos ->*RL_cursorPos
	
	<-rasterHeight 3 sub <-*RL_lineHeight div 1 sub ->*RL_logLength
	
	@ReprintOutput
	
:Backspace
	Substring(<-inputString dup StringLength 1 sub 0 swap) ->inputString
	SendMsg("RL_PrintString" " ")

:InitVarSandbox
	CreateTable ->sandboxedVars


:self
	if(<-scope gte0)
		<-scope
	else
		-1
	endif
	
:runas
	if(StackSize 1 eq)
		->arg1
		if(<-arg1 GetType "STRING" eq)
			-3 ->scope
			<-arg1 ->scopeScript
			@PrintToOutput("Running as global script registered to " DQ <-scopeScript DQ concat4)
		else if(<-arg1 GetType "INT" eq)
			<-arg1 ->scope
			"" ->scopeScript
			@PrintToOutput("Running as unit " <-scope " (official name " DQ concat4 <-scope GetUnitOfficialName DQ concat4 ")" LF "No script specified, using Script1." concat4)
		endif endif
	else
		->arg2 ->arg1
		<-arg1 ->scope
		<-arg2 ->scopeScript
		@PrintToOutput("Running as unit " <-scope " (official name " DQ concat4 <-scope GetUnitOfficialName DQ concat4 ")" LF "Script: " concat4 <-scopeScript concat)
	endif

:refread
	->varName
	
	if(<-varName "*" StartsWith) #global var read
		<-varName dup StringLength 1 sub 1 swap Substring <-!*
	else if(<-scope -1 eq) #default, sandboxed var table
		<-sandboxedVars{<-varName}
	else if(<-scope -2 eq) #debug, directly read internal vars
		<-varName <-!
	else if(<-scope -3 eq) #registered script
		PlaySound("Notify" 1)
		<-scopeScript <-varName GetRegisteredScriptVar 
	else #Simulate running as the unit with the given ID
		<-scope <-scopeScript GetScriptVar
	endif endif endif endif

:refwrite
	->varName

	if(<-varName "*" StartsWith) #global var read
		<-varName dup StringLength 1 sub 1 swap Substring ->!*
	else if(<-scope -1 eq) #default, sandboxed var table
		->sandboxedVars{<-varName}
	else if(<-scope -2 eq) #debug, directly read internal vars
		<-varName ->!
	else if(<-scope -3 eq) #registered script
		<-scopeScript <-varName SetRegisteredScriptVar
	else #Simulate running as the unit with the given ID
		<-scope <-scopeScript SetScriptVar
	endif endif endif endif

:selected
	GetSelectedUnits[0]

:allselected
	GetSelectedUnits

:help
	if(StackSize eq0)
		@PrintToOutput(<-helpTable{"_INTRO_"})
	else
		ToLower ->topic
		@PrintToOutput(<-helpTable{<-topic})
	endif

